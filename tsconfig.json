{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/pages/form/page.ts",
    "app/pages/hur-vi-gor/page.js",
    "app/layout.jsx",
    "app/page.js",
    "components/EmailBtn.js",
    "components/Navbar.js",
    "components/Hero.js",
    "components/Features.js",
    "components/Camp.js",
    "components/Footer.js",
    "components/Guide.js",
    "components/GetApp.js",
    "components/Guide2.js",
    "app/pages/form/page.js",
    "constants/index.js"
  ],
  "exclude": ["node_modules"]
}
